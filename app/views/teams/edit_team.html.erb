<% provide(:title, "Edit Team Information") %>
<div class="edit-team-boarder">
  <h1>Update your team info</h1>

  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <%= form_with(model: @team, local: true) do |f| %>

        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control' %>

        <%= f.label :desc %>
        <%= f.text_field :desc, class: 'form-control' %>

        <%= f.submit "Save changes", class: "btn btn-primary" %>
      <% end %>
    </div>
  </div>
</div class="edit-team-boarder">